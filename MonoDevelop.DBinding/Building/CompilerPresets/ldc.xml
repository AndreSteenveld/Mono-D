<?xml version="1.0" encoding="utf-8"?>
<Compiler>
	<VersionId>LDC</VersionId>

	<TargetConfiguration Target="Executable">
		<CompilerCommand>ldc</CompilerCommand>
		<LinkerCommand>ldc</LinkerCommand>
		<Patterns>
			<obj>"{0}"</obj>
			<include>-I"{0}"</include>
			<version>-d-version</version>
			<debug>-d-debug</debug>
			<unittest>-unittest</unittest>
			<ddFlag>-D</ddFlag>
			<ddMacroDefinition>"{0}"</ddMacroDefinition>
			<ddDir>"-Dd{0}"</ddDir>
		</Patterns>
		<DebugArgs>
			<CompilerArg>-c "$src" -o "$obj" $includes -g</CompilerArg>
			<LinkerArgs>-o "$target" -g $objs $libs</LinkerArgs>
			<OneStepBuildArgs>-O0 -g $sources $libs $includes -o "$exe"</OneStepBuildArgs>
			<gdcLibPrefixing>true</gdcLibPrefixing>
		</DebugArgs>
		<ReleaseArgs>
			<CompilerArg>-c "$src" -release -o "$obj" $includes -O3</CompilerArg>
			<LinkerArgs>-o "$target" -release -O3 $objs $libs</LinkerArgs>
			<OneStepBuildArgs>-O3 -release $sources $libs $includes -o "$exe"</OneStepBuildArgs>
			<gdcLibPrefixing>true</gdcLibPrefixing>
		</ReleaseArgs>
	</TargetConfiguration>

	<TargetConfiguration Target="SharedLibrary">
		<CompilerCommand>ldc</CompilerCommand>
		<LinkerCommand>ldc</LinkerCommand>
		<Patterns>
			<obj>"{0}"</obj>
			<include>-I"{0}"</include>
			<version>-d-version</version>
			<debug>-d-debug</debug>
			<unittest>-unittest</unittest>
			<ddFlag>-D</ddFlag>
			<ddMacroDefinition>"{0}"</ddMacroDefinition>
			<ddDir>"-Dd{0}"</ddDir>
		</Patterns>
		<DebugArgs>
			<CompilerArg>-fPIC -c "$src" -o "$obj" $includes -g</CompilerArg>
			<LinkerArgs>-o "$target" -g $objs $libs -shared</LinkerArgs>
			<gdcLibPrefixing>true</gdcLibPrefixing>
		</DebugArgs>
		<ReleaseArgs>
			<CompilerArg>-fPIC -release -c "$src" -o "$obj" $includes -O3</CompilerArg>
			<LinkerArgs>-o "$target" -release -O3 $objs $libs -shared</LinkerArgs>
			<gdcLibPrefixing>true</gdcLibPrefixing>
		</ReleaseArgs>
	</TargetConfiguration>

	<TargetConfiguration Target="StaticLibrary">
		<CompilerCommand>ldc</CompilerCommand>
		<LinkerCommand>ar</LinkerCommand>
		<Patterns>
			<obj>"{0}"</obj>
			<include>-I"{0}"</include>
			<version>-d-version</version>
			<debug>-d-debug</debug>
			<unittest>-unittest</unittest>
			<ddFlag>-D</ddFlag>
			<ddMacroDefinition>"{0}"</ddMacroDefinition>
			<ddDir>"-Dd{0}"</ddDir>
		</Patterns>
		<DebugArgs>
			<CompilerArg>-c "$src" -o "$obj" $includes -g</CompilerArg>
			<LinkerArgs>rcs "$target" $objs</LinkerArgs>
			<gdcLibPrefixing>true</gdcLibPrefixing>
		</DebugArgs>
		<ReleaseArgs>
			<CompilerArg>-c "$src" -o "$obj" $includes -release -O3</CompilerArg>
			<LinkerArgs>rcs "$target" $objs</LinkerArgs>
			<gdcLibPrefixing>true</gdcLibPrefixing>
		</ReleaseArgs>
	</TargetConfiguration>
</Compiler>